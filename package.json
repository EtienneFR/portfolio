		"svelte-simple-modal": "^1.5.2",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"cookie": "^0.5.0"
